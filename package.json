{
  "name": "jupyter",
  "displayName": "Jupyter",
  "description": "",
  "version": "1.0.0",
  "publisher": "donjayamanne",
  "engines": {
    "vscode": "^1.6.0"
  },
  "keywords": [
    "python",
    "jupyter",
    "ipython"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.sayHello",
    "onCommand:jupyter2.runSelectionLine",
    "onCommand:jupyter2.execCurrentCell",
    "onCommand:jupyter2.execCurrentCellAndAdvance"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      },
      {
        "command": "jupyter2.runSelectionLine",
        "title": "Run Selection/Line",
        "category": "Jupyter2"
      },
      {
        "command": "jupyter2.execCurrentCell",
        "title": "Run Cell",
        "category": "Jupyter2"
      },
      {
        "command": "jupyter2.execCurrentCellAndAdvance",
        "title": "Run Cell and Advance",
        "category": "Jupyter2"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./ && webpack",
    "compile": "webpack && tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/jquery": "^1.10.31",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "@types/rx": "^2.5.33",
    "@types/socket.io": "^1.4.27",
    "@types/socket.io-client": "^1.4.27",
    "@types/uuid": "^3.3.27",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "ignore-loader": "^0.1.1",
    "mocha": "^2.3.3",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "webpack": "^1.13.2"
  },
  "dependencies": {
    "@jupyterlab/services": "^0.34.2",
    "anser": "^1.1.0",
    "node-static": "^0.7.9",
    "requirejs": "^2.3.2",
    "rx": "^4.1.0",
    "socket.io": "^1.4.8",
    "tmp": "0.0.31",
    "transformime": "^3.2.0",
    "transformime-marked": "0.0.1",
    "vscode-extension-telemetry": "0.0.5",
    "ws": "^1.1.1",
    "xml2js": "^0.4.17",
    "xmlhttprequest": "^1.8.0"
  }
}